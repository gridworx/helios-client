    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^6.4.13",
