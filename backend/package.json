    "@modelcontextprotocol/sdk": "^1.24.3",
    "@types/cron": "^2.0.1",
    "@types/handlebars": "^4.0.40",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.4.7",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^4.3.4",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "papaparse": "^5.5.3",
    "pg": "^8.11.3",
    "redis": "^4.7.1",
